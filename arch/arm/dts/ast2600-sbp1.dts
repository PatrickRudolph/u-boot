
#include "ast2600-fpga.dts"

/ {
	model = "IBM SBP1";
	compatible = "ibm,sbp1-bmc", "aspeed,ast2600";

	chosen {
		stdout-path = &uart1;
	};

	aliases {
		ethernet1 = &mac1;
	};
};

&uart1 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&uart5 {
	status = "disabled";
};

&mdio {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_mdio1_default &pinctrl_mdio2_default>;
	ethphy1: ethernet-phy@1 {
		reg = <0>;
	};
};

&mac1 {
	status = "okay";
	phy-mode = "rgmii-rxid";
	phy-handle = <&ethphy1>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_rgmii2_default>;
};

&fmc {
	status = "okay";
	flash@0 {
		compatible = "spansion,s25fl512s", "spi-flash";
		status = "okay";
		spi-max-frequency = <50000000>;
		spi-tx-bus-width = <2>;
		spi-rx-bus-width = <2>;
	};

	flash@1 {
		compatible = "spansion,s25fl512s", "spi-flash";
		status = "okay";
		spi-max-frequency = <50000000>;
		spi-tx-bus-width = <2>;
		spi-rx-bus-width = <2>;
	};
};

&emmc_slot0 {
	status = "disabled";
};

&i2c0 {
	status = "disabled";
};

&i2c3 {
	status = "disabled";
};

&i2c7 {
	status = "disabled";
};

&hace {
	u-boot,dm-pre-reloc;
	status = "okay";
};
