#include "ast2500-evb.dts"

/ {
	model = "IBM Genesis3";
	compatible = "ibm,genesis3-bmc", "aspeed,ast2500";
};

&spi1 {
	status = "disabled";
};

&fmc {
	flash@0 {
		compatible = "spansion,s25fl256l", "spi-flash";
	};

	flash@1 {
		compatible = "spansion,s25fl256l", "spi-flash";
	};
};

